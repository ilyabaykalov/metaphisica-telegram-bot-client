{
  "name": "metaphysica-telegram-bot-client",
  "version": "1.0.0",
  "type": "module",
  "author": {
    "name": "Ilya Baykalov",
    "email": "ibaykalov@blogic.com"
  },
  "scripts": {
    "start": "webpack serve --mode development",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "lint:fix": "eslint \"**/*.{js,ts,tsx}\" --fix",
    "build": "webpack --mode production",
    "postbuild": "cp public/logo.png dist && install /dev/null dist/start.sh && echo \"npx serve -s . -l 3000\" >> dist/start.sh",
    "start:prod": "npm run build && dist/start.sh",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist -b release --no-history --message 'Создание сборки'",
    "redeploy": "sh ../redeploy.sh"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.14",
    "@mui/material": "^5.15.14",
    "@reduxjs/toolkit": "^2.2.3",
    "axios": "^1.6.8",
    "classnames": "^2.5.1",
    "js-file-downloader": "^1.1.25",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.3",
    "react-use-scroll-direction": "^0.1.0",
    "socket.io-client": "^4.7.5"
  },
  "devDependencies": {
    "@types/react": "^18.2.71",
    "@types/react-dom": "^18.2.22",
    "@types/telegram-web-app": "^7.2.1",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.10.0",
    "css-modules-typescript-loader": "^4.0.1",
    "dotenv-webpack": "^8.1.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-amd": "^1.1.0",
    "eslint-plugin-compat": "^4.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.34.1",
    "file-loader": "^6.2.0",
    "gh-pages": "^6.1.1",
    "html-webpack-plugin": "^5.6.0",
    "sass": "^1.72.0",
    "sass-loader": "^14.1.1",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.3",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  }
}
